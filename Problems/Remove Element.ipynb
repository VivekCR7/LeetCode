    "def removeElement(nums, val):\n",
    "    i = 0\n",
    "    for x in range(0,len(nums)):\n",
    "        if nums[x] != val:\n",
    "            nums[i] = nums[x]\n",
    "            i += 1\n",
    "    return i\n",
    "\n",
    "l = [int(i) for i in input().split()]\n",
    "val = int(input())\n",
    "removeElement(l,val)"
   ]
